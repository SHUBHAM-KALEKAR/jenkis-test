pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'This is new Build Stage.'
            }
        }
        stage('Test') {
            steps {
                echo 'This is new Test Stage.'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is new Deploy Stage.'
            }
        }
    }
}
